{
  "functions": {
    "api/index.py": {
      "includeFiles": "app/**,requirements.txt,frontend/**",
      "excludeFiles": "**/__pycache__/**,**/*.pyc,**/*.pyo,**/*.pyd,**/*.md,**/*.txt,app/database/schema.sql,**/tests/**,**/test_*.py,**/*_test.py,**/.git/**,**/.github/**,**/README.md,**/docs/**,frontend/**/*.map,frontend/**/*.min.js.map"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/star-interview.html",
      "destination": "/api/index.py"
    },
    {
      "source": "/star-guidelines.html",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS, PATCH"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-Requested-With"
        }
      ]
    }
  ]
}
